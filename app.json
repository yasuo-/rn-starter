{
  "expo": {
    "name": "React Native Starter Lite",
    "description": "OS version of React Native Starter",
    "slug": "react-native-starter-lite",
    "primaryColor": "#555CC4",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android"],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "assetBundlePatterns": ["assets/**/**"],
    "splash": {
      "image": "./assets/images/background.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "jp.ibj.rnstater"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "android": {
      "package": "jp.ibj.rnstater",
      "versionCode": 1
    }
  }
}
